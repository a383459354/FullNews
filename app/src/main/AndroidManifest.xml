<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.zh.fullnews">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.fullnews.frament.VideoActivity" />
        <activity android:name="com.fullnews.frament.DiscoverActivity" />
        <activity android:name="com.fullnews.frament.PersonageActivity" />
        <activity android:name="com.fullnews.frament.HomeActivity" />
        <activity android:name="com.fullnews.frament.RedactNavigaionItemActivity" />
        <activity android:name="com.fullnews.frament.NewsContentActivity" />
        <activity android:name="com.fullnews.frament.RegisterActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>